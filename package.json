{
  "name": "systemjust-test-FuncButton",
  "version": "1.0.3",
  "description": "FuncButton component, An easy inline-block button",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "example",
    "src"
  ],
  "scripts": {
    "example": "node server.js",
    "build": "npm run build_asset && cross-env BABEL_ENV=commonjs babel src --out-dir dist",
    "build_asset": "build-asset \"src/css/**/*\" -i src/css -o dist -n css",
    "test": "jest Storyshots.test.js --coverage",
    "updateShapshot": "jest --updateSnapshot",
    "storyshotsTest": "storyshots",
    "storybook": "start-storybook -p 9001",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johnmausfam/FuncButton.git"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.11",
    "@storybook/addon-knobs": "^3.3.11",
    "@storybook/addon-links": "^3.3.11",
    "@storybook/addon-options": "^3.3.11",
    "@storybook/addon-storyshots": "^3.3.11",
    "@storybook/addons": "^3.3.11",
    "@storybook/react": "^3.3.11",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "build-asset": "^2.0.6",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.8",
    "file-loader": "^1.1.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.1.4",
    "jest-cli": "^22.1.4",
    "jest-css-modules": "^1.1.0",
    "node-sass": "^4.7.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.2.0",
    "storybook-readme": "^3.2.0",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.0"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "prop-types": "^15.6.0"
  },
  "keywords": [
    "react",
    "component",
    "button",
    "inline-block"
  ],
  "author": "fam",
  "license": "ISC",
  "dependencies": {},
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
